<!DOCTYPE html>
<html lang="en">
<head>
	<%- include('../layout/comman_menu') %>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/lightslider/1.1.6/css/lightslider.min.css"
		integrity="sha512-yJHCxhu8pTR7P2UgXFrHvLMniOAL5ET1f5Cj+/dzl+JIlGTh5Cz+IeklcXzMavKvXP8vXqKMQyZjscjf3ZDfGA=="
		crossorigin="anonymous" referrerpolicy="no-referrer" />
    <script src="https://kit.fontawesome.com/9ae01dec74.js" crossorigin="anonymous"></script>
</head>

<body>
	<%- include('../layout/menu') %>

	<div>
		<div class="container">
			<div class="row">
                <div class="mb-20 main-section">
					<%- contactData[0].main_heading %>
				</div>

			</div>
		</div>
	</div>

	<div class="contact">
		<div class="container">
			<div class="row p-0 count_section">
				<div class="col-lg-3 col-md-6 pl-0">
					<h4>
						<span>250M+</span>
					</h4>
					<p>
						API requests per day, peaking at 13000 requests a second
					</p>
				</div>

				<div class="col-lg-3 col-md-6 pl-0">
					<h4>
						<span>90%</span>
					</h4>
					<p>
						of U.S. adultus have bought from businesses using Stripe. 
					</p>
				</div>

				<div class="col-lg-3 col-md-6 pl-0">
					<h4>
						<span>135+</span>
					</h4>
					<p>
						curriencies and payment method supported.
					</p>
				</div>

				<div class="col-lg-3 col-md-6 pl-0">
					<h4>
						<span>35+</span>
					</h4>
					<p>
						countries with local acquiring, optimising acceptance rates. 
					</p>
				</div>

			</div>
		</div>
	</div>
    <div class="container" id="contact_form_section">
        <div class="row">

            <div class="col-lg-6 col-md-6">
                <div class="row box-contact">
                    <div class="col-md-12 p-0">
                        <!-- Rounded tabs -->
                        <ul id="myTab" role="tablist" class="nav nav-tabs nav-pills flex-column flex-sm-row text-center bg-light border-0 rounded-nav tab-enquiry">
                            <li class="nav-item flex-sm-fill">
                              <a id="sales-tab" data-toggle="tab" href="#sales" role="tab" aria-controls="sales" aria-selected="true" class="nav-link border-0 text-uppercase font-weight-bold active" >Sales Enquiry</a>
                            </li>
                            <li class="nav-item flex-sm-fill">
                              <a id="general-tab" data-toggle="tab" href="#general" role="tab" aria-controls="general" aria-selected="false" class="nav-link border-0 text-uppercase font-weight-bold">General Enquiry</a>
                            </li>
                          </ul>
                          
                        <div id="myTabContent" class="tab-content">
                            <div id="sales" role="tabpanel" aria-labelledby="sales-tab"
                                class="tab-pane fade px-4 py-3 show active">
                                <form id="formSales" method="post" onsubmit="return false;">
                                    <div class="row">

                                        <div class="col-md-12">
                                            <!--<h5>Personal Details</h5>-->
                                            <div class="form-group"></div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!--<label for="first_name">First Name</label>-->
                                                <input type="text" class="form-control" name="first_name"
                                                    id="sales_first_name" placeholder="First Name" required="required">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!--<label for="last_name">Last Name</label>-->
                                                <input type="text" class="form-control" name="last_name"
                                                    id="sales_last_name" placeholder="Last Name" required="required">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!--<label for="mobile">Mobile</label>-->
                                                <input type="text" class="form-control" name="mobile" id="sales_mobile"
                                                    placeholder="Mobile" required="required">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!--<label for="email">Email</label> -->
                                                <input type="email" class="form-control" name="email" id="sales_email"
                                                    placeholder="Email" required="required">
                                            </div>
                                        </div>

                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!--	<label for="Business Name">Business Name</label> -->
                                                <input type="text" class="form-control" name="business_name"
                                                    id="sales_business_name" placeholder="Business Name"
                                                    required="required">
                                            </div>
                                        </div>
                                        
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!-- <label for="Business Address">Business Address</label> -->
                                                <input type="text" class="form-control" name="business_address"
                                                    id="sales_business_address" placeholder="Business Address"
                                                    required="required">
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row mt-20">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!-- <label for="business">Business/Property</label> -->
                                                <select class="form-control" name="business_type" id="sales_business_type">
                                                    <option value="">Select your business</option>
                                                    <option>Car Wash</option>
                                                    <option>Laundromat</option>
                                                    <option>Dog Wash</option>
                                                    <option>Vending</option>
                                                    <option>Parking</option>
                                                    <option>Other</option>
                                                </select>
                                            </div>
                                        </div>
                                        
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!--<label for="Number of Credit Card Terminals Required">Number of Credit Card Terminals Required</label>-->
                                                <input type="number" class="form-control" name="num_of_credit"
                                                    id="sales_num_of_credit"
                                                    placeholder="Number of Credit Card Terminals Required"
                                                    required="required">
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <!--	<label for="Message">Message</label>-->

                                                <textarea class="form-control" name="enquiry_message" id="sales_enquiry_message" rows="4" cols="50"
                                                    placeholder="Message" required="required"></textarea>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                             <div class="alert" role="alert" id="alertTypeSales"></div>
                                        </div>
                                        <div class="col-md-12 mt-20 text-center">
                                            <input type="hidden" name="enquiry_type" value="Sales">
                                            <button class="btn btn-dark  btn-submit" type="submit" id="btnSalesProcced">Proceed <i
                                                    class="fa fa-arrow-right" aria-hidden="true"></i></button>
                                        </div>
                                    </div>

                                </form>

                            </div>

                            <div id="general" role="tabpanel" aria-labelledby="general-tab"
                                class="tab-pane fade px-4 py-3">
                                <form id="formGeneral" method="post" onsubmit="return false;">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group"></div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="first_name"
                                                    id="general_first_name" placeholder="First Name" required="required">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!--<label for="last_name">Last Name</label>-->
                                                <input type="text" class="form-control" name="last_name"
                                                    id="general_last_name" placeholder="Last Name" required="required">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!--<label for="mobile">Mobile</label>-->
                                                <input type="text" class="form-control" name="mobile" id="mobile"
                                                    placeholder="Mobile" required="required">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <!--<label for="email">Email</label> -->
                                                <input type="email" class="form-control" name="email" id="general_email"
                                                    placeholder="Email" required="required">
                                            </div>
                                        </div>
                            
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <!--	<label for="Business Name">Business Name</label> -->
                                                <input type="text" class="form-control" name="business_name"
                                                    id="general_business_name" placeholder="Business Name"
                                                    required="required">
                                            </div>
                                        </div>
                                        
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <!-- <label for="Business Address">Business Address</label> -->
                                                <input type="text" class="form-control" name="business_address"
                                                    id="general_business_address" placeholder="Business Address"
                                                    required="required">
                                            </div>
                                        </div>
                                        
                                        


                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <!--	<label for="Message">Message</label>-->

                                                <textarea class="form-control" name="enquiry_message" rows="4" cols="50" id="general_enquiry_message"
                                                    placeholder="Message" required="required"></textarea>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="alert" role="alert" id="alertTypeGeneral"></div>
                                       </div>
                                        <div class="col-md-12 mt-20 text-center">
                                            <input type="hidden" name="enquiry_type" value="General">
                                            <button class="btn btn-dark  btn-submit" type="submit" id="btnGeneralProcced">Proceed <i
                                                    class="fa fa-arrow-right" aria-hidden="true"></i></button>
                                        </div>
                                    </div>
                                </form>

                            </div>

                        </div>
                        <!-- End rounded tabs -->

                    </div>

                </div>

            </div>
            <div class="col-lg-1 d-none d-lg-block"></div>
            <div class="col-lg-5 col-md-6 contact_us">

                <div class="my_contact">
                    <div class="row mb-20 contact-info contact-top">
                        <div class="col-md-3"> Call Us: </div>
                        <div class="col-md-9"><%= contactData[0].phone_no %></div>
                    </div>
                    </br>
                    <div class="row contact-info">
                        <div class="col-md-3">Email Us:</div>
                        <div class="col-md-9"><%= contactData[0].email %></div>
                    </div>
                    </br>
                    <%- contactData[0].office_details %>
                </div>
            </div>
        </div>
    </div>

	<div class="get_started">
		<div class="container">
			<div class="row">
				<div class="col-lg-6 col_one">
					<h4>Ready to get started?</h4>
					<p>
						Explore <span>Stripe Payments, </span>
						or create an account instantly and start accepting payments,
						You can also contact us to design a custom package for your business.
					</p>
					<div>
						<a class="start_btn" href="">Start Now 
							<i class="fa fa-angle-right" aria-hidden="true"></i>
						</a>
						<a class="sales_btn" href="">Contact Sales 
							<i class="fa fa-angle-right" aria-hidden="true"></i>
						</a>
					</div>
				</div>
				<div class="col-lg-6 col_two">
					<div class="row mt-4 col-lg-0">
						<div class="col-md-6">
							
							<h6>
								<span>Always know what you pay</span>
							</h6>
							<p>Integrated per-transaction pricing with no hidden fees.</p>
							<a class="sales_btn" href="">Pricing details 
								<i class="fa fa-angle-right" aria-hidden="true"></i>
							</a>
						</div>
						<div class="col-md-6">
							
							<h6>
								<span>Start your integration</span>	
							</h6>
							<p>
								Get up and running with Stripe in as little as 10 minutes.
							</p>
							<a class="sales_btn" href="">API reference 
								<i class="fa fa-angle-right" aria-hidden="true"></i>
							</a>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
 <%- include('../layout/footer') %>
</body>

<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/lightslider/1.1.6/js/lightslider.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10/dist/sweetalert2.all.min.js"></script>

<script type="text/javascript">
    $(document).ready(function(){
            $("body").on("submit","form",function(e){
                var formData = new FormData($(this)[0]);
                var formId = $(this).attr('id');
                if(formId == "formSales"){
                var  enquiry_type = "Sales";
                var first_name = $("#sales_first_name").val();
                var last_name = $("#sales_last_name").val();
                var email = $("#sales_email").val();
                var mobile = $("#sales_mobile").val();
                var business_name = $("#sales_business_name").val();
                var business_address = $("#sales_business_address").val();
                var enquiry_message = $("#sales_enquiry_message").val();
                var business_type = $("#sales_business_type").val();
                var sales_num_of_credit = $("#sales_num_of_credit").val();
                $("#btnSalesProcced").html('<i class="fa fa-spinner" aria-hidden="true"></i> Loading...');
                $("#btnSalesProcced").prop("disabled",true);
                }else if(formId == "formGeneral"){
                    var  enquiry_type = "General";
                    var first_name = $("#general_first_name").val();
                    var last_name = $("#general_last_name").val();
                    var email = $("#general_email").val();
                    var mobile = $("#general_mobile").val();
                    var business_name = $("#general_business_name").val();
                    var business_address = $("#general_business_address").val();
                    var enquiry_message = $("#general_enquiry_message").val();
                    var business_type = "";
                    var sales_num_of_credit = "";
                    $("#btnGeneralProcced").html('<i class="fa fa-spinner" aria-hidden="true"></i> Loading...');
                    $("#btnGeneralProcced").prop("disabled",true);
                }
                var sendData = { enquiry_type: enquiry_type, first_name: first_name, last_name: last_name, email: email, mobile: mobile, business_name: business_name, business_address: business_address, enquiry_message: enquiry_message, business_type: business_type, sales_num_of_credit: sales_num_of_credit};
                $.ajax({
                    url: 'send_enquiry',
                    data: sendData,
                    type: 'POST',
                    success: function(data){
                        $('#formSales,#formGeneral')[0].reset();
                        $("#btnSalesProcced,#btnGeneralProcced").html('Proceed <i class="fa fa-arrow-right" aria-hidden="true"></i>');
                        $("#btnSalesProcced,#btnGeneralProcced").prop("disabled",false);
                        if(data == "No"){
                            $("#alertType"+enquiry_type).addClass('alert-danger');
                            $("#alertType"+enquiry_type).html('Something went wrong');
                           
                        }else{
                            $("#alertType"+enquiry_type).addClass('alert-success');
                            $("#alertType"+enquiry_type).html('Your enquiry has been sent successfully.');
                          
                        }
                    }
                });
             });
        });
</script>
</html>