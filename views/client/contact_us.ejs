<script type="text/javascript">
    $(document).ready(function(){
            $("body").on("submit","form",function(e){
                var formData = new FormData($(this)[0]);
                var formId = $(this).attr('id');
                if(formId == "formSales"){
                var  enquiry_type = "Sales";
                var first_name = $("#sales_first_name").val();
                var last_name = $("#sales_last_name").val();
                var email = $("#sales_email").val();
                var mobile = $("#sales_mobile").val();
                var business_name = $("#sales_business_name").val();
                var business_address = $("#sales_business_address").val();
                var enquiry_message = $("#sales_enquiry_message").val();
                var business_type = $("#sales_business_type").val();
                var sales_num_of_credit = $("#sales_num_of_credit").val();
                $("#btnSalesProcced").html('<i class="fa fa-spinner" aria-hidden="true"></i> Loading...');
                $("#btnSalesProcced").prop("disabled",true);
                }else if(formId == "formGeneral"){
                    var  enquiry_type = "General";
                    var first_name = $("#general_first_name").val();
                    var last_name = $("#general_last_name").val();
                    var email = $("#general_email").val();
                    var mobile = $("#general_mobile").val();
                    var business_name = $("#general_business_name").val();
                    var business_address = $("#general_business_address").val();
                    var enquiry_message = $("#general_enquiry_message").val();
                    var business_type = "";
                    var sales_num_of_credit = "";
                    $("#btnGeneralProcced").html('<i class="fa fa-spinner" aria-hidden="true"></i> Loading...');
                    $("#btnGeneralProcced").prop("disabled",true);
                }
                var sendData = { enquiry_type: enquiry_type, first_name: first_name, last_name: last_name, email: email, mobile: mobile, business_name: business_name, business_address: business_address, enquiry_message: enquiry_message, business_type: business_type, sales_num_of_credit: sales_num_of_credit};
                $.ajax({
                    url: 'send_enquiry',
                    data: sendData,
                    type: 'POST',
                    success: function(data){
                        $("#btnSalesProcced,#btnGeneralProcced").html('Proceed <i class="fa fa-arrow-right" aria-hidden="true"></i>');
                        $("#btnSalesProcced,#btnGeneralProcced").prop("disabled",false);
                        if(data == "Yes"){
                            Swal.fire(
                            'Success',
                            'Your enquiry has been sent successfully.',
                            'success'
                            )
                        }else{
                            Swal.fire({
                                icon: 'error',
                                title: 'Oops...',
                                text: 'Something went wrong!',
                            })
                        }
                    }
                });
             });
        });
</script>
