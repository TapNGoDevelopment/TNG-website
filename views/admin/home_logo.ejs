<!DOCTYPE html>
<html lang="en">
<head>
 <%- include('layout/comman_menu') %>
</head>

<body>
 <%- include('layout/menu') %>
  <!-- ======= Sidebar ======= -->
  <%- include('layout/sidebar') %>
    <!-- End Sidebar-->

    <main id="main" class="main">
     
       
        <section class="section">
            <div class="row">
                
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                      <h5 class="card-title">Home Logo</h5>
                      <table class="table">
                        <thead>
                          <tr>
                            <th scope="col">#</th>
                            <th scope="col">Logo</th>
                            <th scope="col">Link</th>
                            <th scope="col">Action</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% if(homeLogo.length!=0){
                            var i = 1; 
                            homeLogo.forEach(function(logos){
                          %>
                          <tr>
                            <th scope="row"><%= i %></th>
                            <td><img src="/home_logo/<%=logos.logo %>" width="100" height="50"/></td>
                            <td><a href="<%=logos.logo_link %>" target="_new"><%=logos.logo_link %></a></td>
                            <td> <button type="button" class="btn btn-sm btn-danger"><i class="bi bi-trash"></i></button></td>
                          </tr>
                          <% i++; }) } %>
                        </tbody>
                      </table>
                    </div>
                  </div>
            </div>
            </div>
        </section>
      </main><!-- End #main -->

  <!-- ======= Footer ======= -->
  <%- include('layout/footer') %>
  <script src="/admin-theme/js/home_page.js"></script>
</body>
</html>